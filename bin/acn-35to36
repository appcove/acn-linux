#!/bin/bash


yum remove -y python35u-mod_wsgi

yum install -y \
	python36u.x86_64 \
	python36u-devel.x86_64 \
	python36u-libs.x86_64 \
	python36u-pip.noarch \
	python36u-redis.noarch \
	python36u-setuptools.noarch \
	python36u-tkinter.x86_64 \
	python36u-tools.x86_64     


acn-setup-modwsgi
acn-setup-bpython



